	Journal = {Molecular Genetics and Metabolism},
	Keywords = {Alu repeats,genetic diversity,human disease,insertion mutation,recombination},
	Langid = {english},
	Number = {3},
	Pages = {183--193},
	Shortjournal = {Molecular Genetics and Metabolism},
	Title = {Alu {{Repeats}} and {{Human Disease}}},
	Url = {https://www.sciencedirect.com/science/article/pii/S1096719299928640},
	Urldataa = {2021-09-28},
