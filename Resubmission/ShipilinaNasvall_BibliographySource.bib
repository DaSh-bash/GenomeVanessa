	Shorttitle = {Vagility across {{Vanessa}} ({{Lepidoptera}}},
	Title = {Vagility across {{Vanessa}} ({{Lepidoptera}}: {{Nymphalidae}}): Mobility in Butterfly Species Does Not Inhibit the Formation and Persistence of Isolated Sister Taxa},
	Url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-3113.2010.00566.x},
