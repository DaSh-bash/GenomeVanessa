	Issn = {0737-4038, 1537-1719},
	Journal = {Molecular Biology and Evolution},
	Langid = {english},
	Number = {11},
	Pages = {2536--2547},
	Title = {De {{Novo Mutation Rate Estimation}} in {{Wolves}} of {{Known Pedigree}}},
