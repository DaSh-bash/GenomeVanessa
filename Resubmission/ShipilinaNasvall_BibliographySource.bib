	Issn = {0737-4038, 1537-1719},
	Journal = {Molecular Biology and Evolution},
	Langid = {english},
	Number = {12},
	Pages = {2842--2853},
	Title = {Butterfly {{Mimicry Polymorphisms Highlight Phylogenetic Limits}} of {{Gene Reuse}} in the {{Evolution}} of {{Diverse Adaptations}}},
