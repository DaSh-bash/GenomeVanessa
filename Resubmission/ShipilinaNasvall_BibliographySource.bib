	Issn = {1545-7885},
	Journal = {PLOS Biology},
	Langid = {english},
	Number = {1},
	Pages = {e1002353},
	Shortjournal = {PLoS Biol},
	Title = {Evolutionary {{Novelty}} in a {{Butterfly Wing Pattern}} through {{Enhancer Shuffling}}},
