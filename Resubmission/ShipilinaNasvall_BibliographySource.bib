	Doi = {10.1111/mec.16272},
	File = {/Library/Application Support/Zotero/storage/A2LIXK4H/Rosser m. fl. - Complex basis of hybrid female sterility and Halda.pdf;/Library/Application Support/Zotero/storage/YVXU2HNH/Rosser m. fl. - 2022 - Complex basis of hybrid female sterility and Halda.pdf;/Library/Application Support/Zotero/storage/NQY2AP8Q/mec.html;/Library/Application Support/Zotero/storage/ZLWYI6RC/mec.html},
	Issn = {1365-294X},
	Journal = {Molecular Ecology},
	Keywords = {Dobzhansky--Muller incompatibilities,Haldane's rule,hybrid sterility,lepidoptera,speciation,ZW sex determination},
	Langid = {english},
	Number = {31},
	Shorttitle = {Complex Basis of Hybrid Female Sterility and {{Haldane}}'s Rule in {{Heliconius}} Butterflies},
	Title = {Complex Basis of Hybrid Female Sterility and {{Haldane}}'s Rule in {{Heliconius}} Butterflies: {{Z-linkage}} and Epistasis},
