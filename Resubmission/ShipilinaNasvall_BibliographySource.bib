	Issn = {2056-3744},
	Journal = {Evolution Letters},
	Keywords = {Chromosomal evolution,evolutionary genomics,insects,speciation},
	Langid = {english},
	Number = {3},
	Pages = {138--154},
	Title = {No Evidence for Maintenance of a Sympatric {{Heliconius}} Species Barrier by Chromosomal Inversions},
