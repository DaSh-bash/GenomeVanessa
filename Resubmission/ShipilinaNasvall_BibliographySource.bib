	File = {/Library/Application Support/Zotero/storage/QQK39IJL/Yang m. fl. - 2020 - Chromosome‐level reference genome assembly and gen.pdf},
	Issn = {1755-098X, 1755-0998},
	Journal = {Molecular Ecology Resources},
	Langid = {english},
	Number = {4},
	Pages = {1080--1092},
	Shortjournal = {Mol Ecol Resour},
	Title = {Chromosome‐level Reference Genome Assembly and Gene Editing of the Dead‐leaf Butterfly {{{\emph{Kallima}}}}{\emph{ Inachus}}},
	Url = {https://onlinelibrary.wiley.com/doi/10.1111/1755-0998.13185},
	Urldataa = {2021-09-28},
