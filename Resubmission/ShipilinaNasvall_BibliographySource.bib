	Journal = {G3\&amp;\#58; Genes|Genomes|Genetics},
	Langid = {english},
	Number = {1},
	Pages = {299--309},
	Shortjournal = {G3},
	Title = {Inferring the {{Genomic Landscape}} of {{Recombination Rate Variation}} in {{European Aspen}} ( {{{\emph{Populus}}}}{\emph{ Tremula}} )},
	Url = {http://g3journal.org/lookup/doi/10.1534/g3.119.400504},
