	Journal = {Proceedings of the National Academy of Sciences},
	Langid = {english},
	Number = {45},
	Pages = {E7030-E7038},
	Shortjournal = {Proc Natl Acad Sci USA},
	Title = {{{RNA-binding}} Profiles of {{{\emph{Drosophila}}}} {{CPEB}} Proteins {{Orb}} and {{Orb2}}},
