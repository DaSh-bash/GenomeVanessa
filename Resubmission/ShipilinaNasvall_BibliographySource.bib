	Publisher = {{Royal Society}},
	Shortjournal = {Proceedings of the Royal Society B: Biological Sciences},
	Shorttitle = {Social Modularity},
	Title = {Social Modularity: Conserved Genes and Regulatory Elements Underlie Caste-Antecedent Behavioural States in an Incipiently Social Bee},
	Url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2019.1815},
