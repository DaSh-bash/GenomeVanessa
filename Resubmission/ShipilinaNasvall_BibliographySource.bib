	Journal = {Nucleic Acids Research},
	Keywords = {Base Sequence,Codon,Escherichia coli,Genes; Bacterial,Genes; Regulator,Protein Biosynthesis,Protein Sorting Signals,RNA; Messenger},
	Langid = {english},
	Number = {19},
	Pages = {7737--7749},
	Pmcid = {PMC311793},
	Shortjournal = {Nucleic Acids Res.},
	Title = {Codon Usage in Regulatory Genes in {{Escherichia}} Coli Does Not Reflect Selection for 'rare' Codons},
