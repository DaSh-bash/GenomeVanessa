	Issn = {1664-302X},
	Journal = {Frontiers in Microbiology},
	Langid = {english},
	Pages = {663},
	Shortjournal = {Front. Microbiol.},
	Title = {Metagenomic {{Sequencing}} of {{Diamondback Moth Gut Microbiome Unveils Key Holobiont Adaptations}} for {{Herbivory}}},
