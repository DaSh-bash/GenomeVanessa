	Issn = {1471-2148},
	Journal = {BMC Evolutionary Biology},
	Langid = {english},
	Number = {1},
	Pages = {59},
	Shortjournal = {BMC Evol Biol},
	Title = {Evolutionary History of Host Use, Rather than Plant Phylogeny, Determines Gene Expression in a Generalist Butterfly},
