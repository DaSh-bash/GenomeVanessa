	File = {/Library/Application Support/Zotero/storage/AR62S2RI/Blackmon m. fl. - 2019 - Meiotic drive shapes rates of karyotype evolution .pdf;/Library/Application Support/Zotero/storage/ES2JFVAF/evo.html},
	Issn = {1558-5646},
	Journal = {Evolution},
	Keywords = {Chromosome number,meiosis,meiotic drive,probabilistic phylogenetic models},
	Langid = {english},
	Number = {3},
	Pages = {511--523},
	Title = {Meiotic Drive Shapes Rates of Karyotype Evolution in Mammals},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/evo.13682},
