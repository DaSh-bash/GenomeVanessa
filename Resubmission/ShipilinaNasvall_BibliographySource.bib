	Issn = {1759-6653},
	Issue = {evab054},
	Journal = {Genome Biology and Evolution},
	Shortjournal = {Genome Biology and Evolution},
	Title = {Chromosome Level Assembly of the Comma Butterfly ({{Polygonia}} c-Album)},
	Url = {https://doi.org/10.1093/gbe/evab054},
