	Doi = {10.1111/mec.12810},
	File = {/Library/Application Support/Zotero/storage/2W9QPMTL/Kawakami m. fl. - 2014 - A high-density linkage map enables a second-genera.pdf},
	Issn = {09621083},
	Journal = {Molecular Ecology},
	Langid = {english},
	Number = {16},
	Pages = {4035--4058},
	Shortjournal = {Mol Ecol},
	Title = {A High-Density Linkage Map Enables a Second-Generation Collared Flycatcher Genome Assembly and Reveals the Patterns of Avian Recombination Rate Variation and Chromosomal Evolution},
	Url = {http://doi.wiley.com/10.1111/mec.12810},
	Urldataa = {2020-04-30},
