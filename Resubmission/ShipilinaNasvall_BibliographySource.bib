	Shortjournal = {J Evolution Biol},
	Title = {Dynamics of Host Plant Use and Species Diversity in {{Polygonia}} Butterflies ({{Nymphalidae}})},
	Url = {http://doi.wiley.com/10.1111/j.1420-9101.2005.01009.x},
