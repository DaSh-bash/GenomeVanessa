	Langid = {english},
	Pages = {jeb212241},
	Shortjournal = {J Exp Biol},
	Title = {How to Study Enhancers in Non-Traditional Insect Models},
	Url = {http://jeb.biologists.org/lookup/doi/10.1242/jeb.212241},
	Urldataa = {2021-04-02},
