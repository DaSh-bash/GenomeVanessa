	Issn = {1367-4811},
	Journal = {Bioinformatics (Oxford, England)},
	Keywords = {Alleles,Data Interpretation; Statistical,Gene Frequency,Genetic Association Studies,Genetics; Population,Genotype,Humans,Mutation,Polymorphism; Single Nucleotide,Sequence Analysis; DNA},
	Langid = {english},
