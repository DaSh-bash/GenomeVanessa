@article{bomanEffectsGCBiasedGene2021,
	Abstract = {Recombination reshuffles the alleles of a population through crossover and gene conversion. These mechanisms have considerable consequences on the evolution and maintenance of genetic diversity. Crossover, for example, can increase genetic diversity by breaking the linkage between selected and nearby neutral variants. Bias in favor of G or C alleles during gene conversion may instead promote the fixation of one allele over the other, thus decreasing diversity. Mutation bias from G or C to A and T opposes GC-biased gene conversion (gBGC). Less recognized is that these two processes may---when balanced---promote genetic diversity. Here, we investigate how gBGC and mutation bias shape genetic diversity patterns in wood white butterflies (Leptidea sp.). This constitutes the first in-depth investigation of gBGC in butterflies. Using 60 resequenced genomes from six populations of three species, we find substantial variation in the strength of gBGC across lineages. When modeling the balance of gBGC and mutation bias and comparing analytical results with empirical data, we reject gBGC as the main determinant of genetic diversity in these butterfly species. As alternatives, we consider linked selection and GC content. We find evidence that high values of both reduce diversity. We also show that the joint effects of gBGC and mutation bias can give rise to a diversity pattern which resembles the signature of linked selection. Consequently, gBGC should be considered when interpreting the effects of linked selection on levels of genetic diversity.},
	Author = {Boman, Jesper and Mugal, Carina F and Backstr{\"o}m, Niclas},
	Date = {2021-05-01},
	Doi = {10.1093/gbe/evab064},
	File = {/Library/Application Support/Zotero/storage/SSK7WCCT/Boman m. fl. - 2021 - The Effects of GC-Biased Gene Conversion on Patter.pdf;/Library/Application Support/Zotero/storage/MVDIS9TU/6184863.html},
	Issn = {1759-6653},
	Journal = {Genome Biology and Evolution},
	Number = {5},
	Pages = {evab064},
	Shortjournal = {Genome Biology and Evolution},
	Title = {The {{Effects}} of {{GC-Biased Gene Conversion}} on {{Patterns}} of {{Genetic Diversity}} among and across {{Butterfly Genomes}}},
	Url = {https://doi.org/10.1093/gbe/evab064},
	Urldataa = {2022-03-17},
	Volume = {13},
	Bdsk-Url-1 = {https://doi.org/10.1093/gbe/evab064}}

@article{bookerVariationRecombinationRate2020,
	Abstract = {Genome scans can potentially identify genetic loci involved in evolutionary processes such as local adaptation and gene flow. Here, we show that recombination rate variation across a neutrally evolving genome gives rise to mixed sampling distributions of mean FST (F{\^S}T), a common population genetic summary statistic. In particular, we show that in regions of low recombination the distribution of F{\^S}T estimates has more variance and a longer tail than in more highly recombining regions. Determining outliers from the genome-wide distribution without taking local recombination rate into consideration may therefore increase the frequency of false positives in low recombination regions and be overly conservative in more highly recombining ones. We perform genome scans on simulated and empirical Drosophila melanogaster data sets and, in both cases, find patterns consistent with this neutral model. Similar patterns are observed for other summary statistics used to capture variation in the coalescent process. Linked selection, particularly background selection, is often invoked to explain heterogeneity in F{\^S}T across the genome, but here we point out that even under neutrality, statistical artefacts can arise due to variation in recombination rate. Our results highlight a flaw in the design of genome-scan studies and suggest that without estimates of local recombination rate, interpreting the genomic landscape of any summary statistic that captures variation in the coalescent process will be very difficult.},
	Author = {Booker, Tom R. and Yeaman, Sam and Whitlock, Michael C.},
	Date = {2020-11},
	Doi = {10.1111/mec.15501},
	File = {/Library/Application Support/Zotero/storage/IXFEPJQM/Booker m. fl. - 2020 - Variation in recombination rate affects detection .pdf},
	Issn = {0962-1083, 1365-294X},
	Journal = {Molecular Ecology},
	Langid = {english},
	Number = {22},
	Pages = {4274--4279},
	Shortjournal = {Mol Ecol},
	Title = {Variation in Recombination Rate Affects Detection of Outliers in Genome Scans under Neutrality},
	Url = {https://onlinelibrary.wiley.com/doi/10.1111/mec.15501},
	Urldataa = {2020-11-17},
	Volume = {29},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/10.1111/mec.15501},
	Bdsk-Url-2 = {https://doi.org/10.1111/mec.15501}}

