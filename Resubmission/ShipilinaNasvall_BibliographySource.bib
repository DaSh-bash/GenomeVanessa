	Shortjournal = {Plant Cell Environ},
	Shorttitle = {Co-Option of Microbial Associates by Insects and Their Impact on Plant-Folivore Interactions},
	Title = {Co-Option of Microbial Associates by Insects and Their Impact on Plant-Folivore Interactions: {{Herbivore}} Co-Option of Microbes},
