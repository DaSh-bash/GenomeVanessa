	Issn = {1476-4687},
	Journal = {Nature},
	Keywords = {Adaptation; Physiological,Alleles,Animals,Biological Evolution,Cell Cycle,Color,DNA Transposable Elements,Evolutionary genetics,Gene expression,Genes; Insect,Haplotypes,Introns,Male,Melanosis,Moths,Mutagenesis; Insertional,Mutation,Phenotype,Pigmentation,Selection; Genetic,United Kingdom,Wings; Animal},
	Langid = {english},
