@article{higgsCoevolutionCodonUsage2008,
	Abstract = {The typical number of tRNA genes in bacterial genomes is around 50, but this number varies from under 30 to over 120. We argue that tRNA gene copy numbers evolve in response to translational selection. In rapidly multiplying organisms, the time spent in translation is a limiting factor in cell division; hence, it pays to duplicate tRNA genes, thereby increasing the concentration of tRNA molecules in the cell and speeding up translation. In slowly multiplying organisms, translation time is not a limiting factor, so the overall translational cost is minimized by reducing the tRNAs to only one copy of each required gene. Translational selection also causes a preference for codons that are most rapidly translated by the current tRNAs; hence, codon usage and tRNA gene content will coevolve to a state where each is adapted to the other. We show that there is often more than one stable coevolved state. This explains why different combinations of tRNAs and codon bias can exist for different amino acids in the same organism. We analyze a set of 80 complete bacterial genomes and show that the theory predicts many of the trends that are seen in these data.},
	Author = {Higgs, Paul G. and Ran, Wenqi},
	Date = {2008-11},
	Doi = {10.1093/molbev/msn173},
	Eprint = {18687657},
	Eprinttype = {pmid},
	File = {/Library/Application Support/Zotero/storage/KGU9VNHT/Higgs och Ran - 2008 - Coevolution of codon usage and tRNA genes leads to.pdf},
	Issn = {1537-1719},
	Journal = {Molecular Biology and Evolution},
	Keywords = {Amino Acids,Anticodon,Bacteria,Codon,Evolution; Molecular,Gene Dosage,Genome; Bacterial,Models; Genetic,Protein Biosynthesis,RNA; Bacterial,RNA; Transfer,Selection; Genetic},
	Langid = {english},
	Number = {11},
