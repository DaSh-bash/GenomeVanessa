	Pages = {4737--4754},
	Shortjournal = {Mol Ecol},
	Shorttitle = {Stacks 2},
	Title = {Stacks 2: {{Analytical}} Methods for Paired‐end Sequencing Improve {{RADseq}}‐based Population Genomics},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/mec.15253},
