	File = {/Library/Application Support/Zotero/storage/YB6CLSLL/Huang m. fl. - 2008 - The UDP-glucosyltransferase multigene family in Bo.pdf},
	Issn = {1471-2164},
	Journal = {BMC Genomics},
	Pages = {563},
	Pmcid = {PMC2633020},
	Shortjournal = {BMC Genomics},
