	Journal = {EvoDevo},
	Langid = {english},
	Number = {1},
	Pages = {10},
	Shortjournal = {EvoDevo},
	Title = {Transcription Factors Underlying Wing Margin Color Patterns and Pupal Cuticle Markings in Butterflies},
