	Pages = {evab283},
	Shortjournal = {Genome Biology and Evolution},
	Shorttitle = {Expanding the {{Menu}}},
	Title = {Expanding the {{Menu}}: {{Are Polyphagy}} and {{Gene Family Expansions Linked}} across {{Lepidoptera}}?},
