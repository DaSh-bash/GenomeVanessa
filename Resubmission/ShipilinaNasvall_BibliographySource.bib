	Issn = {1759-6653},
	Journal = {Genome Biology and Evolution},
	Langid = {english},
	Number = {10},
	Pages = {2875--2886},
	Title = {Dissecting the {{Effects}} of {{Selection}} and {{Mutation}} on {{Genetic Diversity}} in {{Three Wood White}} ({{Leptidea}}) {{Butterfly Species}}},
