	Issn = {1573-6849},
	Journal = {Chromosome Research},
	Langid = {english},
	Number = {7},
	Pages = {733--739},
	Shortjournal = {Chromosome Res},
	Title = {Holocentric Chromosomes in Meiosis. {{I}}. {{Restriction}} of the Number of Chiasmata in Bivalents},
	Url = {https://doi.org/10.1023/B:CHRO.0000045797.74375.70},
