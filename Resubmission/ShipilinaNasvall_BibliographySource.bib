	File = {/Library/Application Support/Zotero/storage/I72GUBHM/Talla et al. - 2017 - Rapid Increase in Genome Size as a Consequence of .pdf},
	Issn = {1759-6653},
	Journal = {Genome Biology and Evolution},
	Langid = {english},
	Number = {10},
	Pages = {2491--2505},
	Title = {Rapid {{Increase}} in {{Genome Size}} as a {{Consequence}} of {{Transposable Element Hyperactivity}} in {{Wood-White}} ({{Leptidea}}) {{Butterflies}}},
