@article{lucekEvolutionaryMechanismsVarying2018,
	Abstract = {The evolution of intrinsic barriers to gene flow is a crucial step in the process of speciation. Chromosomal changes caused by fusion and fission events are one such barrier and are common in several groups of Lepidoptera. However, it remains unclear if and how chromosomal changes have contributed to speciation in this group. I tested for a phylogenetic signal of varying chromosome numbers in Erebia butterflies by combining existing sequence data with karyological information. I also compared different models of trait evolution in order to infer the underlying evolutionary mechanisms. Overall, I found significant phylogenetic signals that are consistent with non-neutral trait evolution only when parts of the mitochondrial genome were included, suggesting cytonuclear discordances. The adaptive evolutionary model tested in this study consistently outperformed the neutral model of trait evolution. Taken together, these results suggest that, unlike other Lepidoptera groups, changes in chromosome numbers may have played a role in the diversification of Erebia butterflies.},
	Author = {Lucek, Kay},
	Date = {2018-03},
	Doi = {10.3390/genes9030166},
	File = {/Library/Application Support/Zotero/storage/42RFLAYC/Lucek - 2018 - Evolutionary Mechanisms of Varying Chromosome Numb.pdf;/Library/Application Support/Zotero/storage/YMLAZ8UN/Lucek - 2018 - Evolutionary Mechanisms of Varying Chromosome Numb.pdf;/Library/Application Support/Zotero/storage/LFZ5TVKR/166.html;/Library/Application Support/Zotero/storage/MGSXN6TC/htm.html},
