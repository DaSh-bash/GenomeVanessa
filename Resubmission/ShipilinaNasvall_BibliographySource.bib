	Issn = {09651748},
	Journal = {Insect Biochemistry and Molecular Biology},
	Langid = {english},
	Pages = {1--11},
	Shortjournal = {Insect Biochemistry and Molecular Biology},
	Title = {Conserved {{RNA}} Cis-Elements Regulate Alternative Splicing of {{Lepidopteran}} Doublesex},
