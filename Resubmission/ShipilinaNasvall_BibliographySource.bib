	File = {/Library/Application Support/Zotero/storage/M9Y4U2CS/Zhu et al. - 2018 - Potential Mechanism of Detoxification of Cyanide C.pdf},
	Issn = {2379-5042},
	Journal = {mSphere},
	Langid = {english},
	Number = {3},
	Pages = {e00229-18, /msphere/3/3/mSphere229-18.atom},
	Shortjournal = {mSphere},
	Title = {Potential {{Mechanism}} of {{Detoxification}} of {{Cyanide Compounds}} by {{Gut Microbiomes}} of {{Bamboo-Eating Pandas}}},
	Url = {http://msphere.asm.org/lookup/doi/10.1128/mSphere.00229-18},
