	Pages = {1804},
	Title = {The {{Landscapes}} of {{Full-Length Transcripts}} and {{Splice Isoforms}} as {{Well}} as {{Transposons Exonization}} in the {{Lepidopteran Model System}}, {{Bombyx}} Mori},
	Url = {https://www.frontiersin.org/article/10.3389/fgene.2021.704162},
