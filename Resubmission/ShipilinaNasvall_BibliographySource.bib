	Shortjournal = {Genome Res.},
	Title = {Genome-Wide Variability in Recombination Activity Is Associated with Meiotic Chromatin Organization},
	Url = {http://genome.cshlp.org/lookup/doi/10.1101/gr.275358.121},
	Urldataa = {2021-09-02},
